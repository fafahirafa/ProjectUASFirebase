<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".NavigationActivity">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <id.ac.pnj.projectuasfirebase.CurvedBottomNavigationView
            android:id="@+id/bottom_nav"
            android:layout_width="match_parent"
            android:layout_height="72dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintEnd_toStartOf="parent"/>
    </android.support.constraint.ConstraintLayout>

        <RelativeLayout
            android:id="@+id/lin_id"
            android:layout_gravity="bottom|center"
            android:layout_margin="40dp"
            android:background="@drawable/oval"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <com.sdsmdg.harjot.vectormaster.VectorMasterView
                android:id="@+id/fab"
                android:layout_centerInParent="true"
                android:layout_margin="12dp"
                android:visibility="gone"
                app:elevation="6dp"
                app:vector_src="@drawable/ic_account_circle_white_24dp"
                android:layout_width="45dp"
                android:layout_height="45dp" />

            <com.sdsmdg.harjot.vectormaster.VectorMasterView
                android:id="@+id/fab1"
                android:layout_centerInParent="true"
                android:layout_margin="12dp"
                app:elevation="6dp"
                app:vector_src="@drawable/ic_control_point_white_24dp"
                android:layout_width="45dp"
                android:layout_height="45dp" />

            <com.sdsmdg.harjot.vectormaster.VectorMasterView
                android:id="@+id/fab2"
                android:layout_centerInParent="true"
                android:layout_margin="12dp"
                android:visibility="gone"
                app:elevation="6dp"
                app:vector_src="@drawable/ic_book_white_24dp"
                android:layout_width="45dp"
                android:layout_height="45dp" />

        </RelativeLayout>



</android.support.constraint.ConstraintLayout>